{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist"
  },
  "include": [
    "./index.ts"
  ],
  "typedocOptions": {
    "entryPoints": [
      "./index.ts",
      "./packages/*"
    ],
    "exclude": [
      "./packages/test-utils"
    ],
    "entryPointStrategy": "packages",
    "out": "documentation"
  }
}
